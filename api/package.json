{
  "name": "trace-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --fix -p . \"./{src,test}/**/*.ts\"",
    "dev": "env-cmd .env cross-env NODE_ENV=develop nodemon --require tsconfig-paths/register --watch src --exec ts-node src/main.ts",
    "test": "env-cmd .env cross-env NODE_ENV=test nyc mocha ./tests/**/*.test.ts --recursive --exit --bail --full-trace --preserve-symlinks --require ts-node/register --require tsconfig-paths/register --require ./tests/utils/configure-mocha.ts --timeout 30000",
    "test:ci": "env-cmd .env cross-env NODE_ENV=test nyc mocha ./tests/unit/**/*.test.ts --recursive --exit --bail --full-trace --preserve-symlinks --require ts-node/register --require tsconfig-paths/register --require ./tests/utils/configure-mocha.ts --timeout 30000",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "ef-tspm",
    "docker-build": "npm run build",
    "start": "npm run build && env-cmd .env cross-env NODE_ENV=develop nodemon && node dist/src/main.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^7.3.2",
    "@nestjs/core": "^7.3.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/testing": "^7.3.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonfile": "^6.0.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.24",
    "nanoid": "^3.1.10",
    "nearley": "^2.19.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "rxjs": "^6.6.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/chai": "^4.2.11",
    "@types/date-fns": "^2.6.0",
    "@types/got": "^9.6.11",
    "@types/jsonfile": "^6.0.0",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^8.0.0",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^14.0.23",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "date-fns": "^2.15.0",
    "env-cmd": "8.0.2",
    "got": "^11.5.2",
    "mocha": "^5.1.0",
    "nock": "^13.0.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^3.9.6"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "text"
    ]
  }
}
